{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0",
  "engines": {
    "node": "^24",
    "pnpm": "^10"
  },
  "imports": {
    "#server/*": "./server/*.js",
    "#web/*": "./src/*.js"
  },
  "scripts": {
    "start-dev": "node --enable-source-maps -r dotenv/config dist/server.js",
    "dev": "vite dev",
    "start-coverage": "node --enable-source-maps instrumented/server.js",
    "start-coverage-dev": "nyc node --enable-source-maps -r dotenv/config instrumented/server.js",
    "build": "cross-env IS_PRERENDER=1 vite build && pnpm build-server",
    "build-server": "tsc --project tsconfig.server.json && tsx scripts/esbuild.server.ts",
    "server-dev": "tsx watch --include \"./server/**/*\" --env-file=.env server.ts",
    "deploy": "pnpm start",
    "analyze": "cross-env ANALYZE=true vite build",
    "test": "vitest run",
    "tsc": "tsc --project tsconfig.json && tsc --project tsconfig.server.json && tsc --project __tests__/tsconfig.json",
    "test:ci": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "lint": "eslint -c eslint.config.mjs --concurrency 4 .",
    "lint:fix": "eslint -c eslint.config.mjs --fix .",
    "cypress": "cd cypress && pnpm run tsc && pnpm run cypress",
    "build-nyc": "pnpm build && tsx scripts/copy-dist-for-nyc.ts && nyc instrument --nycrc-path .nycrc.server.yaml dist instrumented"
  },
  "sideEffects": false,
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
