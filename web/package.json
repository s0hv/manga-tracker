{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0",
  "engines": {
    "node": "^18",
    "pnpm": "^8"
  },
  "scripts": {
    "start": "node --enable-source-maps --experimental-specifier-resolution=node dist/server.js",
    "start-dev": "node --enable-source-maps --experimental-specifier-resolution=node -r dotenv/config dist/server.js",
    "start-coverage": "node --enable-source-maps --experimental-specifier-resolution=node instrumented/server.js",
    "start-coverage-dev": "node --enable-source-maps --experimental-specifier-resolution=node -r dotenv/config instrumented/server.js",
    "build": "pnpm build-server && next build",
    "build-server": "tsc --project tsconfig.server.json && tsc-alias --project tsconfig.server.json",
    "server-dev": "nodemon -r dotenv/config --experimental-specifier-resolution=node --loader ./loader.js --ext \"ts,js\" --watch api/* --watch db/* --watch server.ts server.ts",
    "deploy": "pnpm start",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .jsx,.js,.snap,.ts,.tsx .",
    "cypress": "cypress open",
    "build-start-test": "pnpm build && pnpm start-dev",
    "next-build": "next build",
    "build-nyc": "pnpm build && nyc instrument --compact=false dist instrumented"
  },
  "sideEffects": false,
  "cacheDirectories": [
    ".next/cache/"
  ],
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "exclude": [
      "**/node_modules/**"
    ],
    "excludeAfterRemap": true
  },
