  "scripts": {
    "start": "node --enable-source-maps --import=extensionless/register dist/server.js",
    "start-dev": "node --enable-source-maps --import=extensionless/register -r dotenv/config dist/server.js",
    "start-coverage": "node --enable-source-maps --import=extensionless/register instrumented/server.js",
    "start-coverage-dev": "node --enable-source-maps --import=extensionless/register -r dotenv/config instrumented/server.js",
    "build": "pnpm build-server && next build",
    "build-server": "tsc --project tsconfig.server.json && tsc-alias --project tsconfig.server.json",
    "server-dev": "tsx watch --include \"./server/**/*\" --env-file=.env server.ts",
    "deploy": "pnpm start",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "vitest run",
    "tsc": "tsc --project tsconfig.json && tsc --project tsconfig.server.json && tsc --project __tests__/tsconfig.json",
    "test:ci": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "lint": "eslint -c eslint.config.mjs .",
    "lint:fix": "eslint -c eslint.config.mjs --fix .",
    "cypress": "cd cypress && pnpm run tsc && pnpm run cypress",
    "build-start-test": "pnpm build && pnpm start-dev",
    "next-build": "next build",
    "build-nyc": "pnpm build && nyc instrument --compact=false dist instrumented"
  },
  "sideEffects": false,
  "cacheDirectories": [
    ".next/cache",
    "web/.next/cache"
  ],
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "exclude": [
      "**/node_modules/**"
    ],
    "excludeAfterRemap": true
  },
