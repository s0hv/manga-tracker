{
  "extends": "../tsconfig.server.json",
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "outDir": "dist"
  },
  "include": [
    "../**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "../src/**",
    "../server/*.ts",
    "../__tests__/**",
    "../__mocks__/**",
    "../cypress/**",
    "../*.ts",
    "../server/api/**",
    "../scripts/*.ts"
  ]
}
