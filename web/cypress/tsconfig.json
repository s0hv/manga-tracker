{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "esnext"],
    "types": ["cypress", "@testing-library/cypress", "node"],
    "baseUrl": ".",
    "paths": {
      "@/common*": ["common*"],
      "@/serverUtils*": ["../utils*"],
      "@/db*": ["db*"],
      "@/types*": ["types*"]
    }
  },
  "include": ["**/*.ts", "../cypress.config.ts"],
  "exclude": [
    "node_modules"
  ],
      "paths": {
      "@/common*": ["common*"],
      "@/serverUtils*": ["../utils*"],
      "@/db*": ["db*"],
      "@/types*": ["types*"]
    }
}
